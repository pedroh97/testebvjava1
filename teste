public class main {
    public static void main(String[] args) {

        System.out.println("teste !!");

    }

}

class Cadastro {

    private int idade;
    private String nome;
    private char sexo;

    public Cadastro(int idade,String nome,char sexo){
        
    } 


    public void idade() {
        idade = 3;
    }

    public void nome() {
        nome = "Pedro";
    }
    
    public void sexo() {
        sexo = 'M';
    }


}

